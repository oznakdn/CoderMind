@model IEnumerable<CoderMind.Shared.Dtos.TechnologyDtos.GetTechnologySubjectsDto>

@foreach (var item in Model)
{
    <div class="accordion-item" style="margin-left:10px;">
        <h2 class="accordion-header" id="headingOne">
            <button class="accordion-button" type="button" data-bs-toggle="collapse" data-bs-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                <div class="header_img w-25"> <img src="@item.Logo" class="img-fluid rounded rounded-circle"></div>
            </button>
        </h2>
        <div id="collapseOne" class="accordion-collapse collapse" aria-labelledby="headingOne" data-bs-parent="#accordionExample">
            <a href="#" class="btn btn-primary btn-sm btn-block mt-2 mx-2"><i class='bx bxs-add-to-queue'></i> Subject</a>
            <div class="accordion-body">
                @foreach (var item2 in item.Subjects)
                {
                    <a href="#" class="link-primary">@item2.Title</a>
                    <hr />
                }

            </div>
        </div>
    </div>
}

